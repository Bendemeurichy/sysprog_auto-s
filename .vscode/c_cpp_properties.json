{
    "configurations": [
        {
            "name": "GCC",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/libraries/SDL2/x86_64-w64-mingw32/include/SDL2",
                "${workspaceFolder}/libraries/glmc/include",
                "/usr/include/SDL2",
                "/usr/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
