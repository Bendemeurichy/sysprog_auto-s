void load_hardcoded_level_0(Level* level) {
   level->nr = 0;
   level->name = "A Trivial Start";
   level->source_file_name = "game/level/level_files/level0.blvl";
   level->width = 27;
   level->height = 30;
   level->item_count = 26;
   level->points = hardcoded_level_points_0;
   level->items = hardcoded_level_items_0;
}


