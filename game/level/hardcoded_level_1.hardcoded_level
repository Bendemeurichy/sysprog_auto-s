void load_hardcoded_level_1(Level* level) {
   level->nr = 1;
   level->name = "Level 1: The Right Hand Rule";
   level->source_file_name = "game/level/level_files/level1.blvl";
   level->width = 27;
   level->height = 30;
   level->item_count = 17;
   level->points = hardcoded_level_points_1;
   level->items = hardcoded_level_items_1;
}


