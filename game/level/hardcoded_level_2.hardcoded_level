void load_hardcoded_level_2(Level* level) {
   level->nr = 2;
   level->name = "First Filters";
   level->source_file_name = "game/level/level_files/level2.blvl";
   level->width = 27;
   level->height = 24;
   level->item_count = 46;
   level->points = hardcoded_level_points_2;
   level->items = hardcoded_level_items_2;
}


